	  <item>
        <tag>NOW</tag>
        <one-of>
          <item>now</item>
		  <item>start</item>
		  <item>hello</item>
		  <item>play</item>
        </one-of>
      </item>
	   <item>
        <tag>JOINN</tag>
        <one-of>
          <item>join</item>
		  <item>joins</item>
		  <item>joinroom</item>
        </one-of>
      </item>
	   <item>
        <tag>CREATEE</tag>
        <one-of>
          <item>create</item>
		  <item>creates</item>
		  <item>createroom</item>
        </one-of>
      </item>
	   <item>
        <tag>AII</tag>
        <one-of>
          <item>ai</item>
		  <item>ais</item>
		  <item>playai</item>
        </one-of>
      </item>
	   <item>
        <tag>OPTIONN</tag>
        <one-of>
          <item>option</item>
		  <item>options</item>
        </one-of>
      </item>
	   <item>
        <tag>QUITT</tag>
        <one-of>
          <item>quits</item>
		  <item>quit</item>
		  <item>exit</item>
        </one-of>
      </item>
      <item>
        <tag>MASTER</tag>
        <one-of>
          <item>master</item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>